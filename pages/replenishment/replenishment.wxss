page{
    background: #f2f2f2;
}

.container{
    height: 100%;
}

.head{
    width: 100%;
    height: 90rpx;
    background: white;
}

.search{
    width: 100%;
    height:90rpx;
    display: flex;
    align-items: center;
    justify-content: space-around;
}
.search input{
    border:2rpx solid rgba(94,94,94,1);
    border-radius:34px;
    width:400rpx;
    height: 68rpx;
    padding-left: 20rpx;
    box-sizing: border-box;
    font-size: 26rpx;
    text-align: center;
}

.search view{
    font-size: 30rpx;
    background: #595CA1;
    color: white;
    padding: 10rpx 20rpx;
    border-radius: 30rpx;
}

/*主要部分*/
.replenishment_item {
    width: 690rpx;
    height:270rpx;
    background:rgba(255,255,255,1);
    border-radius:8rpx;
    margin: 20rpx auto;
}


.mainBottom{
    height: calc(100% - 90rpx);
}

.replenishment_item .item-title{
    height: 80rpx;
    font-size:30rpx;
    color:rgba(51,51,51,1);
    line-height: 80rpx;
    padding: 0 30rpx;
    box-sizing: border-box;
    border-bottom: 1rpx solid #f2f2f2;
}

.replenishment_item .item-title .order{
    width: 35rpx;
    height: 40rpx;
    vertical-align: middle;
    margin-right: 15rpx;
}

.replenishment_item .item-title .deviceDetails{
    width: 49rpx;
    height: 51rpx;
    vertical-align: middle;
    float: right;
    margin-top: 15rpx;
}

.replenishment_item .item_time {
    font-size: 26rpx;
    line-height: 60rpx;
    padding-left: 32rpx;
}
.replenishment_item .item_bottom{
    width: 100%;
    display: flex;
    margin-top: 15rpx;
}

.replenishment_item .item_bottom>view{
    flex: 1;
    font-size:26rpx;
    color:rgba(153,153,153,1);
    line-height: 50rpx;
}

.replenishment_item .item_bottom>view>view:last-child{
    color:rgba(51,51,51,1);
    font-size: 30rpx;
}


.replenishment_item .item_bottom .item_name{
    padding-left:32rpx;
}




.mainBottom_refresh,.mainBottom_loadMore{
    text-align: center;
    font-size: 32rpx;
}

.picker{
    line-height: 60rpx;
    border-bottom: 1px solid #f2f2f2;
}


